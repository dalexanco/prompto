{
  "name": "react-typescript-chrome-extension-starter",
  "version": "2.0.0",
  "description": "Web Extension starter kit built with React, TypeScript, Tailwind CSS, EsLint, Prettier & Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack -w --config webpack.dev.js",
    "test": "jest --config=jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config=jest.config.js",
    "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
    "prettify": "prettier --write \"src/**/*.ts*\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeksco/react-typescript-chrome-extension-starter.git"
  },
  "keywords": [
    "react",
    "typescript",
    "chrome",
    "extension",
    "boilerplate"
  ],
  "author": "Alexander Schwartzberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeksco/react-typescript-chrome-extension-starter/issues"
  },
  "homepage": "https://github.com/aeksco/react-typescript-chrome-extension-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-interactions": "^7.0.23",
    "@storybook/react": "^7.0.23",
    "@storybook/react-webpack5": "^7.0.23",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/chrome": "^0.0.231",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.195",
    "@types/node": "^14.11.8",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "autoprefixer": "^10.4.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.5.2",
    "babel-loader": "^9.1.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^26.5.3",
    "jest-css-modules": "^2.1.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.2",
    "storybook": "^7.0.23",
    "tailwindcss": "^3.0.8",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "webextension-polyfill": "^0.9.0"
  }
